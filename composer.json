{
  "name": "lyucean/spaced_repetition_bot",
  "description": "Telegraph bot for spaced repetition",
  "type": "library",
  "authors": [
    {
      "name": "lyucean",
      "email": "lyucean@gmail.com"
    }
  ],
  "license": "MIT",
  "minimum-stability": "alpha",
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "ext-curl": "*",
    "thingengineer/mysqli-database-class": "dev-master",
    "ext-mbstring": "*",
    "airbrake/phpbrake": "^0.7.3",
    "lyucean/telegram_bot_php": "dev-forked"
  },
  "autoload": {
    "psr-4": {
      "srbot\\model\\": "model",
      "srbot\\core\\": "core",
      "srbot\\command\\": "command"
    },
    "files": [
      "app/helper.php",
      "app/config.php",
      "app/tracking.php"
    ]
  },
  "config": {
    "optimize-autoloader": true
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "repositories": {
    "lyucean": {
      "type": "vcs",
      "url": "https://github.com/lyucean/telegram_bot_php"
    }
  }
}
